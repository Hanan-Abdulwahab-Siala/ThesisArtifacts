        </table>> ]

    PowerUnit -> Vehicle [headlabel="", taillabel="powerUnits    1..*", label="", arrowhead="odiamond", arrowtail="empty", style="", fontname="Helvetica", fontcolor="black", fontsize=10.0, color="red"];
